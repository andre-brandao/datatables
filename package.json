{
    "name": "@vincjo/datatables",
    "version": "2.0.0",
    "keywords": [
        "svelte sveltejs table tables datatable datatables"
    ],
    "description": "A toolkit for creating datatable components with Svelte",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vincjo/datatables.git"
    },
    "author": "vincjo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vincjo/datatables/issues"
    },
    "homepage": "https://vincjo.fr/datatables",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "package": "svelte-kit sync && svelte-package",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --plugin-search-dir . --write .",
        "deploy": "pm2 deploy ecosystem.config.cjs production"
    },
    "devDependencies": {
        "@sveltejs/adapter-node": "^2.0.0",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/package": "^2.3.1",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@ts-ast-parser/core": "^0.8.0",
        "@types/node": "^18.15.11",
        "gros": "^1.0.1",
        "mdsvex": "^0.11.0",
        "prettier": "^2.6.2",
        "prettier-plugin-svelte": "^2.10.1",
        "prism-svelte": "^0.5.0",
        "prismjs": "^1.29.0",
        "svelte": "^5.0.0-next.148",
        "svelte-check": "^3.4.3",
        "svelte-preprocess": "^5.0.3",
        "tslib": "^2.3.1",
        "typescript": "^5.0.0",
        "typescript-parser": "^2.6.1",
        "vite": "^5.0.0"
    },
    "peerDependencies": {
        "svelte": ">=5.0.0"
    },
    "type": "module",
    "files": [
        "dist"
    ],
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./remote": {
            "types": "./dist/remote/index.d.ts",
            "default": "./dist/remote/index.js"
        }
    },
    "typesVersions": {
        ">4.0": {
            "index.d.ts": [
                "./dist/index.d.ts"
            ],
            "local/Comparator": [
                "./dist/client/Comparator.d.ts"
            ],
            "local/Context": [
                "./dist/client/Context.d.ts"
            ],
            "local/DataHandler": [
                "./dist/client/DataHandler.d.ts"
            ],
            "local/Datatable.svelte": [
                "./dist/client/Datatable.svelte.d.ts"
            ],
            "local/handlers/FilterHandler": [
                "./dist/client/handlers/FilterHandler.d.ts"
            ],
            "local/handlers/PageHandler": [
                "./dist/client/handlers/PageHandler.d.ts"
            ],
            "local/handlers/SearchHandler": [
                "./dist/client/handlers/SearchHandler.d.ts"
            ],
            "local/handlers/SelectHandler": [
                "./dist/client/handlers/SelectHandler.d.ts"
            ],
            "local/handlers/SortHandler": [
                "./dist/client/handlers/SortHandler.d.ts"
            ],
            "local": [
                "./dist/client/index.d.ts"
            ],
            "local/Pagination.svelte": [
                "./dist/client/Pagination.svelte.d.ts"
            ],
            "local/RowCount.svelte": [
                "./dist/client/RowCount.svelte.d.ts"
            ],
            "local/RowsPerPage.svelte": [
                "./dist/client/RowsPerPage.svelte.d.ts"
            ],
            "local/Search.svelte": [
                "./dist/client/Search.svelte.d.ts"
            ],
            "local/Th.svelte": [
                "./dist/client/Th.svelte.d.ts"
            ],
            "local/ThFilter.svelte": [
                "./dist/client/ThFilter.svelte.d.ts"
            ],
            "remote/Context": [
                "./dist/remote/Context.d.ts"
            ],
            "remote/DataHandler": [
                "./dist/remote/DataHandler.d.ts"
            ],
            "remote/Datatable.svelte": [
                "./dist/remote/Datatable.svelte.d.ts"
            ],
            "remote/handlers/FilterHandler": [
                "./dist/remote/handlers/FilterHandler.d.ts"
            ],
            "remote/handlers/PageHandler": [
                "./dist/remote/handlers/PageHandler.d.ts"
            ],
            "remote/handlers/SearchHandler": [
                "./dist/remote/handlers/SearchHandler.d.ts"
            ],
            "remote/handlers/SelectHandler": [
                "./dist/remote/handlers/SelectHandler.d.ts"
            ],
            "remote/handlers/SortHandler": [
                "./dist/remote/handlers/SortHandler.d.ts"
            ],
            "remote/handlers/TriggerHandler": [
                "./dist/remote/handlers/TriggerHandler.d.ts"
            ],
            "remote": [
                "./dist/remote/index.d.ts"
            ],
            "remote/Pagination.svelte": [
                "./dist/remote/Pagination.svelte.d.ts"
            ],
            "remote/README.svelte": [
                "./dist/remote/README.svelte.d.ts"
            ],
            "remote/RowCount.svelte": [
                "./dist/remote/RowCount.svelte.d.ts"
            ],
            "remote/RowsPerPage.svelte": [
                "./dist/remote/RowsPerPage.svelte.d.ts"
            ],
            "remote/Search.svelte": [
                "./dist/remote/Search.svelte.d.ts"
            ],
            "remote/Th.svelte": [
                "./dist/remote/Th.svelte.d.ts"
            ],
            "remote/ThFilter.svelte": [
                "./dist/remote/ThFilter.svelte.d.ts"
            ]
        }
    }
}
