<script>
    import { check } from '@vincjo/datatables'
    export let handler

    handler.filter(11, 'id', check.isEqualTo)
    handler.filter(42, 'age', check.isGreaterThanOrEqualTo)
    handler.filter('joh', 'first_name', check.startsWith)
</script>
